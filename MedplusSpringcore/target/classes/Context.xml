<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
			http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
			http://www.springframework.org/schema/context 
			http://www.springframework.org/schema/context/spring-context-4.0.xsd">
			
			<bean id ="emp" class="core.Employee" scope="prototype" autowire="">
			<property name="name" value="akhil"></property>
			<property name="code" value="111"></property>
			
			
			</bean>
			<bean id ="address" class="core.Address">
			
			<property name="city" value="hyderabad"></property>
			<property name="street" value="madhapur"></property>
			</bean>
			

	
</beans>